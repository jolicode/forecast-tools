{
    "name": "jolicode/harvest-forecast-tools",
    "description": "This project is a collection of tools for Harvest and Forecast: public sharable Forecasts, Slack Forecast reminders, Slack standup meeting reminders, and also a stronger invoicing process.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "adam-paterson/oauth2-slack": "^1.1",
        "antishov/doctrine-extensions-bundle": "^1.4",
        "dragonmantank/cron-expression": "^2.3",
        "jolicode/forecast-php-api": "^4.1",
        "jolicode/harvest-php-api": "^4.1",
        "jolicode/slack-php-api": "dev-fix-jane-minor-version",
        "knpuniversity/oauth2-client-bundle": "^1.26",
        "nilesuan/oauth2-harvest": "^1.2",
        "nyholm/psr7": "^1.3",
        "php-http/curl-client": "^2.1",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/asset": "5.1.*",
        "symfony/console": "5.1.*",
        "symfony/dotenv": "5.1.*",
        "symfony/expression-language": "5.1.*",
        "symfony/flex": "^1.6",
        "symfony/form": "5.1.*",
        "symfony/framework-bundle": "5.1.*",
        "symfony/http-client": "5.1.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/orm-pack": "*",
        "symfony/process": "5.1.*",
        "symfony/security-bundle": "5.1.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "5.1.*",
        "symfony/twig-bundle": "5.1.*",
        "symfony/validator": "5.1.*",
        "symfony/web-link": "5.1.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/workflow": "5.1.*",
        "symfony/yaml": "5.1.*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.16",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.14",
        "symfony/profiler-pack": "*",
        "symfony/test-pack": "*",
        "symfony/web-server-bundle": "~4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
